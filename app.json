{
  "expo": {
    "name": "ottery",
    "slug": "ottery",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Ottery needs access to your Camera.",
        "NSMicrophoneUsageDescription": "Ottery needs access to your Microphone."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.anonymous.ottery"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Ottery to access your camera."
        }
      ],
      "react-native-vision-camera"
    ],
    "web": {},
    "scheme": "ottery"
  }
}
